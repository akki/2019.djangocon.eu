*{padding:0;margin:0;box-sizing:border-box}body,html{width:100%;color:#203341}body{font-family:poppins,sans-serif;font-size:16px}h1{text-align:center;margin-bottom:1rem}h2{color:#2b3a80;text-align:center;margin-bottom:1rem}h3{color:#2b3a80;text-transform:uppercase;text-align:center;margin-bottom:1rem}input[type=email],input[type=text]{border:0;background-color:#e7e7e7;padding:1rem;font-size:16px;font-family:poppins,sans-serif}.button,button,input[type=submit]{padding:1rem;background:#4dc3ce;border:0;font-size:16px;font-family:poppins,sans-serif;-webkit-appearance:none;-moz-appearance:none;appearance:none}p{margin-bottom:1rem}a{color:#4dc3ce}.container{max-width:40rem;margin:0 auto}.header{display:flex;padding:1rem;background:#203341,10%;justify-content:space-between;background-position:50%;background-size:400px;align-items:center;background-image:url(/img/pattern.jpg)}@media(min-width:50rem){.header nav{width:80%}}.header nav ul{display:flex;justify-content:space-around;flex-flow:column;align-items:center}@media(min-width:50rem){.header nav ul{flex-flow:row}}.header nav li{list-style-type:none}.header nav a{font-weight:700;text-align:center;text-decoration:none;text-transform:uppercase;padding:1rem;letter-spacing:1px;line-height:1.15;color:#fff;display:inline-block}.header nav a:hover{background-color:#fff;cursor:pointer;color:#ee585b}.header-logo{width:170px;display:block;margin-left:1rem;margin:.75rem 1rem}.header-logo-square{width:60px}main{margin:4rem 0;padding:0 1rem}main h2{margin-top:2rem}main p{font-size:1.1rem}main ul li{margin-left:1rem;margin-bottom:1rem}footer{color:#fff;display:flex;padding:1rem;margin-top:1rem;background:#203341;background-position:50%;background-image:url(/img/pattern.jpg);background-size:400px;justify-content:space-between;align-items:center;font-size:.8rem;flex-flow:column;text-align:center}footer p{margin:1rem}@media(min-width:50rem){footer{flex-flow:row}}.form-subscribe{text-align:center;margin:2rem 0 4rem}.form-subscribe p{margin-bottom:1.5rem}.page-index .logo{width:70%;display:block;margin:0 auto}.page-index h1{margin:3rem 0;line-height:1.25;color:#2b3a80}.schedule li{list-style-type:none;margin-bottom:1rem;align-items:center}@media(min-width:50rem){.schedule li{display:flex}}.schedule li p{margin:0;width:100%}.schedule li .month{padding-right:1rem;text-transform:uppercase;font-weight:700;color:#ee585b;font-size:.9rem;letter-spacing:1px;display:block}@media(min-width:50rem){.schedule li .month{display:inline-block;flex-basis:8rem;width:8rem;margin-left:-8rem;text-align:right}}.schedule li.strike{text-decoration:line-through;opacity:.6}.schedule li.strike .month{color:#203341}